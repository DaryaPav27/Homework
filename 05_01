//Задание 1

#include <iostream>
using namespace std;

int main()
{
	int a;
	char b;
	cout << "Enter a character:\n";
	cin >> b;
	a = (int)b;
	if (((a >= 65) && (a <= 90)) ||
		((a >= 97) && (a <= 122)))
	{
		cout << "You entered a letter";
	}
	else if ((a >= 48) && (a <= 57))
	{
		cout << "You entered a digit";
	}
	else if ((a == 33) || ((a >= 44) &&
		(a <= 46)) || (a == 58) ||
		(a == 59) || (a == 63))
	{
		cout << "You entered a punctuation mark";
	}
	else
	{
		cout << "You entered an unknown symbol";
	}
}

